{
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true
  },
  "rules": {
    "semi": 2,
    "quotes": [1, "single"],
    "no-underscore-dangle": 0,
    "no-process-exit": 0,
    "camelcase": 0,
    "complexity": [1, 10],
    "no-eq-null": 1,
    "radix": 1,
    "indent": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "no-multiple-empty-lines": [1, {"max": 1}],
    "new-cap": 0,
    "no-shadow": 0
  },
  "env": {
    "node": true
  }
}